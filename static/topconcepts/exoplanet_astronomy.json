ted": null, "changeNotes": null, "scopeNotes": null, "examples": null, "definition": null, "editorialNotes": null}, {"name": "Cold Neptunes", "uri": "http://astrothesaurus.org/uat/2132", "altLabels": null, "related": null, "changeNotes": null, "scopeNotes": null, "examples": null, "definition": null, "editorialNotes": null}, {"name": "Extrasolar gaseous planets", "uri": "http://astrothesaurus.org/uat/2172", "altLabels": ["Extrasolar gas planets"], "related": null, "changeNotes": null, "scopeNotes": null, "examples": null, "definition": null, "editorialNotes": null, "children": [{"name": "Extrasolar gaseous giant planets", "uri": "http://astrothesaurus.org/uat/509", "altLabels": ["Extrasolar gas giants"], "related": [{"name": "Solar system gas giant planets", "uri": "http://astrothesaurus.org/uat/1191"}], "changeNotes": null, "scopeNotes": null, "examples": null, "definition": null, "editorialNotes": null}]}, {"name": "Extrasolar ice giants", "uri": "http://astrothesaurus.org/uat/2024", "altLabels": null, "related": null, "changeNotes": null, "scopeNotes": null, "examples": null, "definition": null, "editorialNotes": null}, {"name": "Extrasolar rocky planets", "uri": "http://astrothesaurus.org/uat/511", "altLabels": ["Rocky planets"], "related": [{"name": "Solar system terrestrial planets", "uri": "http://astrothesaurus.org/uat/797"}], "changeNotes": null, "scopeNotes": null, "examples": null, "definition": null, "editorialNotes": null}, {"name": "Free floating planets", "uri": "http://astrothesaurus.org/uat/549", "altLabels": ["Free-floating planets", "Interstellar planets", "Nomad planets", "Orphan planets", "Rogue planets"], "related": [{"name": "Gravitational microlensing", "uri": "http://astrothesaurus.org/uat/672"}, {"name": "Gravitational microlensing exoplanet detection", "uri": "http://astrothesaurus.org/uat/2147"}], "changeNotes": null, "scopeNotes": null, "examples": null, "definition": "A population of substellar objects which are not bound to stars; they are detected in young star clusters. Their masses, estimated from their fluxes, is several Jupiter masses, lower than those of brown dwarfs. Their formation is not yet explained. Among the envisaged possibilities: 1) These objects form like stars, from protostellar core collapse and subsequent accretion; 2) they form as low-mass members of small groups, and are ejected from the group; 3) they form like planets within circumstellar disks of higher-mass objects, but are ejected either due to internal dynamics or external interactions.", "editorialNotes": [{"title": "Definition Provenance", "comment": "An Etymological Dictionary of Astronomy and Astrophysics, by M. Heydari-Malayeri"}]}, {"name": "Habitable planets", "uri": "http://astrothesaurus.org/uat/695", "altLabels": null, "related": null, "changeNotes": null, "scopeNotes": null, "examples": null, "definition": null, "editorialNotes": null}, {"name": "Hot Jupiters", "uri": "http://astrothesaurus.org/uat/753", "altLabels": ["Epistellar jovians", "Pegasean planets", "Pegasids", "Roaster planets"], "related": null, "changeNotes": null, "scopeNotes": null, "examples": null, "definition": "A giant, gaseous, Jupiter-like planet lying too close to its parent star and having an orbital period from a few days to a few weeks. The existence of hot Jupiters is usually interpreted in terms of planetary migration. These planets can, in principle, be formed at larger distances from their stars and migrate to the inner regions due to dynamical interaction with the proto-planetary disk.", "editorialNotes": [{"title": "Definition Provenance", "comment": "An Etymological Dictionary of Astronomy and Astrophysics, by M. Heydari-Malayeri"}]}, {"name": "Hot Neptunes", "uri": "http://astrothesaurus.org/uat/754", "altLabels": null, "related": null, "changeNotes": null, "scopeNotes": null, "examples": null, "definition": null, "editorialNotes": null}, {"name": "Mini Neptunes", "uri": "http://astrothesaurus.org/uat/1063", "altLabels": null, "related": null, "changeNotes": null, "scopeNotes": null, "examples": null, "definition": null, "editorialNotes": null}, {"name": "Ocean planets", "uri": "http://astrothesaurus.org/uat/1151", "altLabels": ["Water worlds"], "related": null, "changeNotes": null, "scopeNotes": null, "examples": null, "definition": "A hypothetical exoplanet covered by a water envelope. The presence of such a planet stems from the implicit assumption of Habitable Zone temperatures and a liquid water surface.", "editorialNotes": [{"title": "Definition Provenance", "comment": "An Etymological Dictionary of Astronomy and Astrophysics, by M. Heydari-Malayeri"}]}, {"name": "Pulsar planets", "uri": "http://astrothesaurus.org/uat/1304", "altLabels": null, "related": null, "changeNotes": null, "scopeNotes": null, "examples": null, "definition": "A planet orbiting a pulsar. The first such planet to be discovered was around a millisecond pulsar known as PSR 1257+12.", "editorialNotes": [{"title": "Definition Provenance", "comment": "An Etymological Dictionary of Astronomy and Astrophysics, by M. Heydari-Malayeri"}]}, {"name": "Super Earths", "uri": "http://astrothesaurus.org/uat/1655", "altLabels": ["Super-Earths"], "related": null, "changeNotes": null, "scopeNotes": null, "examples": null, "definition": "An extrasolar planet more massive than the Earth but less massive than 10 Earth masses. The first discovered super-Earth orbits an M4 V star named GJ 876. Its estimated mass is 7.5\u00b10.7 Earth masses and it has an orbital period of 1.94 days. It is close to the host star, and the surface temperature is calculated to lie between 430 and 650 K.", "editorialNotes": [{"title": "Definition Provenance", "comment": "An Etymological Dictionary of Astronomy and Astrophysics, by M. Heydari-Malayeri"}]}, {"name": "Trojan planets", "uri": "http://astrothesaurus.org/uat/1716", "altLabels": null, "related": null, "changeNotes": null, "scopeNotes": null, "examples": null, "definition": null, "editorialNotes": null}]}, {"name": "Natural satellites (Extrasolar)", "uri": "http://astrothesaurus.org/uat/483", "altLabels": ["Exomoons"], "related": [{"name": "Exoplanets", "uri": "http://astrothesaurus.org/uat/498"}, {"name": "Natural satellites (Solar system)", "uri": "http://astrothesaurus.org/uat/1089"}], "changeNotes": null, "scopeNotes": null, "examples": null, "definition": "A natural satellite orbiting an extrasolar planet.", "editorialNotes": [{"title": "Definition Provenance", "comment": "An Etymological Dictionary of Astronomy and Astrophysics, by M. Heydari-Malayeri"}], "children": [{"name": "Irregular natural satellites (Extrasolar)", "uri": "http://astrothesaurus.org/uat/2025", "altLabels": ["Irregular exomoons", "Irregular extrasolar natural satellites", "Irregular natural extrasolar satellites", "Irregular moons (Extrasolar)"], "related": [{"name": "Irregular satellites", "uri": "http://astrothesaurus.org/uat/2027"}], "changeNotes": null, "scopeNotes": null, "examples": null, "definition": "A satellite whose orbit around its exoplanet is eccentric, inclined with respect to the equatorial plane, and relatively far from the planet. Strong stellar perturbations cause the orbit to precess.", "editorialNotes": [{"title": "Definition Provenance", "comment": "Modified from An Etymological Dictionary of Astronomy and Astrophysics, by M. Heydari-Malayeri"}]}]}, {"name": "Planet hosting stars", "uri": "http://astrothesaurus.org/uat/1242", "altLabels": null, "related": null, "changeNotes": null, "scopeNotes": null, "examples": null, "definition": null, "editorialNotes": null, "children": [{"name": "Habitable zone", "uri": "http://astrothesaurus.org/uat/696", "altLabels": null, "related": null, "changeNotes": null, "scopeNotes": null, "examples": null, "definition": "A zone around a star where the temperature would be in the range 0-100 \u00b0C to sustain liquid water on the surface of rocky planets (or sufficiently large moons). Water is thought to be a necessary component to the formation and evolution of Earth-type life. This zone depends on the parent star's luminosity and distance; it will be farther from hotter stars. A more accurate definition of a Habitable Zone needs to include other factors, such as orbital eccentricity, heat sources other than stellar irradiation, and atmospheric properties.", "editorialNotes": [{"title": "Definition Provenance", "comment": "An Etymological Dictionary of Astronomy and Astrophysics, by M. Heydari-Malayeri"}]}]}]}